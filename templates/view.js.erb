var <%= object_name %>View = Backbone.View.extend({
  template: JST['<%= underscore_name %>s/view'],
  initialize: function() {
    _.bindAll(this, 'render');
  },
  render: function() {
    $(this.el).html(this.template(this.model.toJSON()));
    this.$('button').text(this.model.isNew() ? 'Create' : 'Update');
    return this;
  }
});

